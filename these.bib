@inproceedings{normand2010spie,
    author = {Normand, Nicolas and
                Guedon, Jeanpierre V. and 
                Philippe, Olivier and 
                Barba, Dominique},
    title = {Controlled redundancy for image coding and high-speed transmission},
    journal = {Proc. SPIE},
    volume = {2727},
    pages = {1070-1081},
    abstract = {The goal of this paper is to describe a new fully reversible image transform specifically designed for image coding and transmission in a context of possible loss of information as encountered in ATM networks. The so-called Mojette transform, is based on a discrete exact radon transform which allows for a natural redundancy of the initial image information. The inverse Mojette transform is particularly flexible and efficient: any portion of the coded image can be replaced by another one if transmission problems occur. Furthermore, the transform is also well suited to the two-layers paradigm used for video transmission on ATM networks by using a multiscale and/or quadtree decomposition into the transform domain. Finally, we discuss the merits of the transform both for image storage and transmission in order to show its ability to operate as the core representation of digital images.},
    year = {1996},
    doi = {10.1117/12.233180},
}

